{
  "name": "browser-vue-loader",
  "version": "0.1.0",
  "description": "Load Vue module",
  "author": "Ivor Xinzi Zhou",
  "license": "MIT",
  "homepage": "https://github.com/imdreamrunner/browser-vue-loader",
  "main": "dist/browser-vue-loader.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "test": "HEDLESS=true karma start --single-run",
    "prepack": "npm run build",
    "lint": "standard --verbose --fix",
    "start-test": "karma start --no-auto-watch",
    "run-test": "karma run --"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:imdreamrunner/browser-vue-loader.git"
  },
  "standard": {
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "ignore": [
      "dist/"
    ]
  },
  "dependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-amd-system-wrapper": "^0.3.7",
    "babel-plugin-transform-cjs-system-wrapper": "^0.6.2",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-standalone": "^6.26.0",
    "css-modules-loader-core": "^1.1.0",
    "es-module-loader": "^2.2.8",
    "generic-names": "^1.0.3",
    "lodash": "^4.17.5",
    "md5": "^2.2.1",
    "postcss": "^6.0.21",
    "postcss-selector-parser": "^3.1.1",
    "postcss-value-parser": "^3.3.0",
    "sass.js": "^0.10.9",
    "string-hash": "^1.1.3",
    "sugarss": "^1.0.1",
    "uuid": "^3.2.1",
    "vue-template-compiler": "^2.5.16",
    "vue-template-es2015-compiler": "^1.6.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^16.1.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.1.4",
    "jquery": "^3.3.1",
    "karma": "^2.0.0",
    "karma-browserify": "^5.2.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "^0.0.32",
    "mocha": "^5.0.5",
    "raw-loader": "^0.5.1",
    "standard": "^11.0.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "watchify": "^3.11.0",
    "webpack": "^4.5.0",
    "webpack-cli": "2.0.13",
    "webpack-dev-server": "3.1.1",
    "webpack-merge": "4.1.2"
  }
}
